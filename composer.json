{
  "name": "efrogg/synergy-bundle",
  "version": "0.1.0",
  "description": "A simple API for JavaScript",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Efrogg\\Synergy\\": "src/"
    },
    "files": []
  },
  "require": {
    "php": ">=8.4.0",
    "symfony/mercure-bundle": "^0.3.8",
    "symfony/ux-turbo": "^2.16",
    "symfony/stimulus-bundle": "^2.0",
    "symfony/serializer": "^7.0|^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "efrogg/synergy-maker-bundle": "allow to generate entities and form for vuetify"
  }
}
